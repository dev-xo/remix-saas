{
  "include": ["./prisma/seed.ts"],
  "compilerOptions": {
    "lib": ["ES2019"],
    "target": "ES2019",
    "moduleResolution": "node",
    "module": "CommonJS",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "outDir": "./prisma/seed",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    }
  }
}
